<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">
	<diskStore path="/home/project/data" />
	<defaultCache eternal="true" maxElementsInMemory="50"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="siteDTO" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="1"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="1" />
		
	<cache name="html" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="9999"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="10000" />
		
	<cache name="ProductNameExisting" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="9999"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="100000" />	
		
	<cache name="setting" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="9999"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="GetSpecialCategoryTree" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="9999"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="GetCategoryTree" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="9999"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="Product" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="TabProduct" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="CategoryDetail" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="ProductCountByCategory" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="ProductSummaryByCategory" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />

	<cache name="allCurrency" eternal="false"
		timeToLiveSeconds="6000" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="LowestPriceInCategory" eternal="false"
		timeToLiveSeconds="120" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
	
	<cache name="HighestPriceInCategory" eternal="false"
		timeToLiveSeconds="120" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
	
</ehcache>
