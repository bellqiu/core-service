<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">
	<diskStore path="/home/project/data" />
	<defaultCache eternal="true" maxElementsInMemory="50"
		memoryStoreEvictionPolicy="LFU" />
	<cache name="siteDTO" eternal="false"
		timeToLiveSeconds="6000" maxElementsInMemory="1"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="1" />
		
	<cache name="SettingServiceCacheWraper_getStringValue" eternal="false"
		timeToLiveSeconds="6000" maxElementsInMemory="9999"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="10000" />
		
	<cache name="ProductNameExisting" eternal="false"
		timeToLiveSeconds="6000" maxElementsInMemory="9999"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="100000" />	
		
	<cache name="HtmlServiceCacheWraper_getStringValue" eternal="false"
		timeToLiveSeconds="6000" maxElementsInMemory="9999"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
		<cache name="Product" eternal="false"
		timeToLiveSeconds="6000" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
</ehcache>
