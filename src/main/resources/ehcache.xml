		maxElementsOnDisk="99999" />
		
	<cache name="ProductSummaryByCategory" eternal="false"
		timeToLiveSeconds="5" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />

	<cache name="allCurrency" eternal="false"
		timeToLiveSeconds="1000" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="GetCurrencyByCode" eternal="false"
		timeToLiveSeconds="1000" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="LowestPriceInCategory" eternal="false"
		timeToLiveSeconds="1000" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
	
	<cache name="HighestPriceInCategory" eternal="false"
		timeToLiveSeconds="1000" maxElementsInMemory="10000"
		memoryStoreEvictionPolicy="LRU" statistics="true" overflowToDisk="true"
		maxElementsOnDisk="99999" />
		
	<cache name="SearchProductName" eternal="false"
